{
  "name": "48tools",
  "version": "2.3.0",
  "description": "48应援工具，提供各种常用功能。",
  "license": "GNU General Public License v3.0",
  "scripts": {
    "dll": "milktea dll",
    "start": "npm run clean:build && cross-env NODE_ENV=development milktea start",
    "build": "npm run clean:build && cross-env NODE_ENV=production milktea build",
    "clean:build": "rimraf ./dist",
    "npm:i": "npm install --production",
    "yarn:i": "yarn install --production=true --pure-lockfile",
    "clean": "node-modules-clean --ext \".opts|.map|.ts|.yml|.png|.dot|.jpg\" --file \"test.js\"",
    "eslint": "eslint ./src --fix",
    "case-check": "case-check --dir=\"./src\" --css",
    "update": "milktea update"
  },
  "main": "./dist/index.html",
  "nodejs": true,
  "window": {
    "title": "48工具（v2.3.0）",
    "position": "center",
    "toolbar": true,
    "frame": true,
    "width": 1200,
    "height": 600,
    "fullscreen": false,
    "show_in_taskbar": true,
    "icon": "./titleBarIcon.png"
  },
  "author": {
    "name": "段昊辰",
    "email": "duanhaochen@126.com",
    "url": "https://github.com/duan602728596"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "fs-extra": "^8.1.0",
    "node-xlsx": "^0.15.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@48/modian-query-sign": "github:duan602728596/modian-query-sign",
    "@sweet-milktea/milktea": "^0.7.1",
    "@sweet-milktea/milktea-cli": "^0.6.7",
    "@sweet-milktea/util-tools": "^0.8.0",
    "antd": "^3.26.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.0",
    "classnames": "^2.2.6",
    "commitizen": "^4.0.3",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "es-module-path-case-check": "^1.4.0",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "flv.js": "^1.5.0",
    "husky": "^3.1.0",
    "immutable": "^4.0.0-rc.12",
    "indexeddb-tools": "^3.0.0",
    "indexeddb-tools-redux": "^3.0.0",
    "jquery": "^3.4.1",
    "lint-staged": "^9.5.0",
    "lodash-es": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "rc-queue-anim": "^1.8.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {
    "node-modules-clean": "^0.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./app/node_modules/cz-conventional-changelog"
    }
  }
}
