    "babel-plugin-import": "^1.9.1",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-react": "^7.11.1",
    "flv.js": "^1.4.2",
    "immutable": "^4.0.0-rc.10",
    "indexeddb-tools": "^2.0.4",
    "indexeddb-tools-redux": "^2.0.0",
    "jquery": "^3.3.1",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.6.2",
    "rc-queue-anim": "^1.6.6",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-loadable": "^5.5.0",
