    "indexeddb-tools": "^2.0.3",
    "indexeddb-tools-redux": "^2.0.0",
    "jquery": "^3.2.1",
    "node-sass": "^4.7.2",
    "nwjs-webpack-hot-loader": "github:duan602728596/nwjs-webpack-hot-loader",
